    "@langchain/core": "^1.1.15",
    "@langchain/langgraph": "^1.1.0",
    "@langchain/openai": "^1.2.2",
    "axios": "^1.7.0",
    "dotenv": "^16.4.5",
    "viem": "^2.21.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vitest": "^2.0.0"
  }
