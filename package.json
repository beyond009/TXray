{
  "name": "mevagent",
  "version": "0.1.0",
  "description": "MEV/EVM Transaction Analysis Agent with LangGraph",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "analyze": "tsx src/cli.ts",
    "test": "vitest",
    "type-check": "tsc --noEmit",
